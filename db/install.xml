<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/newplugin/db" VERSION="20200508" COMMENT="XMLDB file for Moodle local/newplugin"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="np_newtable" COMMENT="This is a newtable">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Name of the thing"/>
        <FIELD NAME="type" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false" COMMENT="Type of the thing"/>
        <FIELD NAME="nplevel" TYPE="int" LENGTH="2" NOTNULL="false" SEQUENCE="false" COMMENT="Level of the thing"/>
        <FIELD NAME="duedate" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>